body {
    cursor: url(https://cdn.jsdelivr.net/gh/tzy13755126023/BLOG_SOURCE/theme_f/curcor1.png), auto;
}

.hide-block>.hide-button.open,
.hide-inline>.hide-button.open {
    display: block
}

a,
button,
img {
    cursor: url(https://cdn.jsdelivr.net/gh/liqincoding/gitee/Mengshen-Handwritten.ttf), auto
}
@font-face {
    font-family:'arzhu';
    src:url('https://cdn.jsdelivr.net/gh/liqincoding/gitee/Mengshen-Handwritten.ttf'); /* 修改成你的字体 */
    font-display:swap
}
h1#site-title {
    font-family:arzhu!important
}
span#subtitle {
    font-family:arzhu!important
}
a#site-name {
    font-family:arzhu!important
}

